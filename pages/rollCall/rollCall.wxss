/* pages/rollCall/rollCall.wxss */
page{
  height: 100%;
}
.rC_page{
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction: column;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
}

.rC_p{
  /* 内容： */
  height: 25%;
  width: 96%;
  /* 边框*/
  border: 1rpx dotted black;
  /* 内边距： */
  /* padding: 40rpx;*/
  margin: 8rpx auto; 
  /* 文字水平方向垂直方向居中 */
  color: #13227a;
  /* 边框做圆角显示 */
  border-radius: 40rpx;
}